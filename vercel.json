{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/templates/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "api/templates/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ]
}